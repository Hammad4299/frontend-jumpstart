    "private": true,
    "scripts": {
        "move:typings": "node webpack/lib/move-typings.js",
        "move:lib": "node webpack/lib/move-lib.js",
        "env:webpack-tsconfig": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\"",
        "webpack:watch": "yarn env:webpack-tsconfig webpack --watch --config webpack/webpack.config.ts",
        "webpack:devserver": "yarn env:webpack-tsconfig webpack serve --config webpack/webpack.config.ts",
        "webpack:build": "yarn env:webpack-tsconfig webpack --config webpack/webpack.config.ts",
        "ttsc:build-declarations": "ttsc --noEmit false --emitDeclarationOnly true",
        "lib:build-declarations": "yarn ttsc:build-declarations -p webpack/lib/tsconfig.json --declarationDir dist/lib/typings && yarn move:typings",
        "lib:build:umd": "yarn webpack:build --config-name lib-umd && yarn move:lib",
        "lib:build:es": "yarn webpack:build --config-name lib-es && yarn move:lib",
        "bit:track": "bit add build-lib/$(git branch --show-current)/umd/shared/* --id test-$(git branch --show-current) && bit add build-lib/$(git branch --show-current)/esm/shared/* --id test-$(git branch --show-current) && bit add build-lib/$(git branch --show-current)/typings/** --id test-$(git branch --show-current)",
        "bit:tag": "bit tag test-$(git branch --show-current)",
        "bit:export": "node webpack/lib/bit-release.js",
        "bit:release": "yarn lib:build:umd && yarn lib:build-declarations && yarn bit:export",
        "test:build": "yarn webpack:build --config-name web-test",
        "test:run": "mocha -r esm --diff tests/**/*.spec.js",
        "test": "yarn test:build && yarn test:run",
        "start": "yarn watch",
        "watch": "yarn webpack:devserver --config-name web-dev",
        "build": "cross-env NODE_ENV=production yarn webpack:build --config-name web-production",
        "lint": "eslint src --ext .js,.ts,.jsx,.tsx,.php",
        "format": "yarn prettier:run",
        "prettier:run": "prettier --write **/*.js **/*.ts **/*.jsx **/*.tsx **/*.json **/*.php **/*.css **/*.sass **/*.scss **/*.less"
    },
